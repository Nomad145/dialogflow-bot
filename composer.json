{
    "name": "michaeljoelphillips/hermes",
    "description": "Hermes, the reliable chatbot",
    "type": "project",
    "require": {
        "php": "^7.4",
        "google/apiclient": "^2.2",
        "jwilsson/spotify-web-api-php": "^3.6",
        "symfony/cache": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
